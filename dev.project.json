{
    "name": "Waiter",

    "tree": {
        "$className": "DataModel",

        "Workspace": {
            "$className": "Workspace",
            "Level1": {
                "$className": "Folder",
                "$properties": {
                    "Tags": ["Level1", "WaiterInstance"],
                    "Attributes": {
                        "WaiterInstanceIds": {"String": "Level1"}
                    }
                },
                "Level2": {
                    "$className": "Folder",
                    "$properties": {
                        "Tags": ["Level2", "WaiterInstance"],
                        "Attributes": {
                            "WaiterInstanceIds": {"String": "Level2"}
                        }
                    },
                    "Level3": {
                        "$className": "Configuration",
                        "$properties": {
                            "Tags": ["Level3", "WaiterInstance"],
                            "Attributes": {
                                "WaiterInstanceIds": {"String": "Level3"}
                            }
                        },
                        "Level1ObjectValue": {
                            "$className": "ObjectValue",
                            "$properties": {
                                "Tags": ["Level1ObjectValue"]
                            }
                        },
                        "Level2ObjectValue": {
                            "$className": "ObjectValue",
                            "$properties": {
                                "Tags": ["Level2ObjectValue"]
                            }
                        },
                        "OtherLevel2ObjectValue": {
                            "$className": "ObjectValue",
                            "$properties": {
                                "Tags": ["Level2ObjectValue", "OtherLevel2ObjectValue"]
                            }
                        },
                        "Level3ObjectValue": {
                            "$className": "ObjectValue",
                            "$properties": {
                                "Tags": ["Level3ObjectValue"]
                            }
                        }
                    }
                },
                "OtherLevel2": {
                    "$className": "Configuration",
                    "$properties": {
                        "Tags": ["Level2", "OtherLevel2", "WaiterInstance"],
                        "Attributes": {
                            "WaiterInstanceIds": {"String": "Other,Level2"}
                        }
                    }
                }
            }
        },

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "Packages": {
                "$path": "Packages",
                "Waiter": {
                    "$path": "src"
                }
            },
            "DevPackages": {
                "$path": "DevPackages"
            }
        },
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$path": "ServerScriptService"
        }
    }
}